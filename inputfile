u=1 # hubbard interaction strength
t=1 # nearest neighbor hopping integral
g=1 # electron-phonon coupling
w=1 # phonon frequency
temp=1 # temperature
Np=1 # number of electrons of a single spin
Ns0=4 # number of sites
e_method='scf' # electronic structure method, 'scf' or 'exact'
trials = 80 # number of trajectories
nprocs = 8 # number of processors
tmax = 10 # max time
dt = 0.1 # output timestep
dt_bath = 0.02 # propagation timestep
